{
  "name": "@immowelt/stylelint-config-immowelt",
  "version": "0.0.0-development",
  "description": "StyleLint standard config for Immowelt",
  "license": "MIT",
  "homepage": "https://github.com/ImmoweltGroup/stylelint-config-immowelt#readme",
  "scripts": {
    "release": "semantic-release --branches main",
    "test": "jest",
    "prettier": "prettier --ignore-path \"../.gitignore\" --write .",
    "prettier:check": "prettier --ignore-path \"../.gitignore\" --check ."
  },
  "dependencies": {
    "@immowelt/browserslist-config": "^3.0.8",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.14.2"
  },
  "devDependencies": {
    "jest": "^27.2.4",
    "prettier": "^2.4.1",
    "semantic-release": "^18.0.0"
  },
  "bugs": {
    "url": "https://github.com/ImmoweltGroup/stylelint-config-immowelt/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ImmoweltGroup/stylelint-config-immowelt.git"
  },
  "keywords": [
    "stylelint",
    "immowelt",
    "config",
    "immonet"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ],
    "analyzeCommits": {
      "releaseRules": [
        {
          "type": "BREAKING",
          "release": "major"
        },
        {
          "type": "FEATURE",
          "release": "minor"
        },
        {
          "type": "BUGFIX",
          "release": "patch"
        },
        {
          "type": "PATCH",
          "release": "patch"
        }
      ]
    }
  },
  "volta": {
    "node": "16.13.1",
    "yarn": "1.22.17"
  }
}
