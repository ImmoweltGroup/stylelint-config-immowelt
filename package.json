{
	"name": "@immowelt/stylelint-config-immowelt",
	"version": "0.0.0-development",
	"description": "StyleLint standard config for Immowelt",
	"license": "MIT",
	"homepage": "https://github.com/ImmoweltGroup/stylelint-config-immowelt#readme",
	"scripts": {
		"release": "semantic-release"
	},
	"dependencies": {
		"@immowelt/browserslist-config": "^2.0.0",
		"stylelint": "^9.10.1",
		"stylelint-config-recommended-scss": "^3.2.0",
		"stylelint-config-standard": "^18.2.0",
		"stylelint-no-unsupported-browser-features": "^3.0.1",
		"stylelint-order": "^1.0.0",
		"stylelint-scss": "^3.14.2"
	},
	"devDependencies": {
		"semantic-release": "^18.0.0"
	},
	"bugs": {
		"url": "https://github.com/ImmoweltGroup/stylelint-config-immowelt/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ImmoweltGroup/stylelint-config-immowelt.git"
	},
	"keywords": [
		"stylelint",
		"immowelt",
		"config",
		"immonet"
	],
	"publishConfig": {
		"access": "public"
	},
	"release": {
		"analyzeCommits": {
			"releaseRules": [
				{
					"type": "BREAKING",
					"release": "major"
				},
				{
					"type": "FEATURE",
					"release": "minor"
				},
				{
					"type": "BUGFIX",
					"release": "patch"
				},
				{
					"type": "PATCH",
					"release": "patch"
				}
			]
		}
	},
	"volta": {
		"node": "14.18.0",
		"yarn": "1.22.15"
	}
}
